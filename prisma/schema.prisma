generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  email     String   @unique
  name      String
  image     String
  createdAt DateTime @default(now())
  provider  Provider
  id    Int    @id @default(autoincrement())
}

enum Provider {
  Google
}